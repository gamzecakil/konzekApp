<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Article List</title>
  </head>
  <body>
    <table border="1">
      <thead>
        <tr>
          <th>Title</th>
          <th>Create Date</th>
          <th>Description</th>
        </tr>
      </thead>
      <tbody>
        <% articles.forEach(article=>{ %>
        <tr>
          <td><%= article.title %></td>
          <td><%= article.createdAt.toLocaleDateString() %></td>
          <td><%= article.description %></td>
        </tr>
        <%}) %>
      </tbody>
    </table>
  </body>
</html>
